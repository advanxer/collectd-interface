rrdtool graph <%= @target %> \
--imgformat <%= @type.upcase %> \
--end <%= @end %> \
--start=<%= @start %> \
--grid-dash 1:1 \
--width=<%= @width %> \
--height=<%= @height %> \
<% if @graph_only %>
--full-size-mode \
--only-graph \
<% end %>
--tabwidth=10 \
--border=0 \
--color=BACK#FFFFFF \
<%= yield %>
